# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# OpenRouter (LLM Gateway)
OPENROUTER_API_KEY=sk-or-your-key
OPENROUTER_MODEL=anthropic/claude-3.5-sonnet

# Mapbox (Geocoding & Maps)
MAPBOX_ACCESS_TOKEN=pk.your-mapbox-token
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=pk.your-mapbox-token

# LiteAPI (Hotel Search & Booking)
# Get from: https://dashboard.liteapi.travel/apikeys/
LITEAPI_SANDBOX_KEY=sand_your-sandbox-key
# LITEAPI_PRODUCTION_KEY=  # Add production key when ready

# Google Places (POI Data) - only needed for POI migration scripts
# Get from: https://console.cloud.google.com/apis/credentials
# GOOGLE_PLACES_API_KEY=your-google-places-api-key

# Foursquare (POI Data) - Legacy, no longer used
# FOURSQUARE_API_KEY=fsq_your-api-key

# Unsplash / Pexels - only needed for image migration scripts
# UNSPLASH_ACCESS_KEY=your-unsplash-key
# PEXELS_API_KEY=your-pexels-key

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Analytics (Optional)
# NEXT_PUBLIC_POSTHOG_KEY=your-posthog-key
# NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com
